<!-- header-starts -->
<div class="header">
  <div class="container">
    <div class="header-info">
      <div class="header-info-head text-center">
        <a href="index.html"><h1><%= t "base_title" %></h1></a>
        <h2><%= t "static_pages.home.welcome" %></h2>
      </div>
      <div class="top-search">
        <%= search_form_for @search, url: search_tours_path, method: :post  do |f| %>
          <%= f.text_field :name_cont, class: "text", value: "Search tour ...",
          onfocus: "this.value = '';", onblur:"if (this.value == '') {this.value = 'Tour or place ...';}" %>
        <% end %>
      </div>
      <div class="clearfix"></div>
      <!-- start h_menu4 -->
        <div class="h_menu4">
        <a class="toggleMenu" href="">Menu</a>
        <ul class="nav">
          <li class="active">
            <%= link_to t("static_pages.home.title"), root_path %>
          </li>
          <li>
            <%= link_to t("static_pages.header.categories"), categories_path %>
            <ul>
              <% @categories.each do |category| %>
                <li><%= link_to category.title, category_path(category) %></li>
              <% end %>
            </ul>
          </li>
          <li>
            <%= link_to t("destination"), places_path %>
            <ul>
              <% @places.each do |place| %>
                <li><%= link_to place.name, place_path(place) %></li>
              <% end %>
            </ul>
          </li>
          <li>
            <%= link_to t("discount-header"), discounts_path %>
            <ul>
              <% @discounts.each do |discount| %>
                <li><%= link_to discount.name, discount_path(discount) %></li>
              <% end %>
            </ul>
          </li>
          <li><%= link_to t("static_pages.contact.title"), static_pages_path(:contact) %></li>
          <% if user_signed_in? %>
            <li><%= link_to current_user.username, user_path(current_user) %></a>
              <ul>
                <li>
                  <%= link_to t("static_pages.header.edit"),
                    edit_user_registration_path %>
                </li>
                <li>
                  <%= link_to t("static_pages.header.logout"),
                    destroy_user_session_path, method: :delete %>
                </li>
              </ul>
            </li>
          <% else %>
            <li><%= link_to t("login.title"), new_user_session_path %></li>
          <% end %>
        </ul>
      </div>
      <!-- end h_menu4 -->
    </div>
  </div>
</div>
<!-- header-ends -->
